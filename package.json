  "engines": {
    "node": "18.16.1",
    "npm": "9.7.2"
  },
  "dependencies": {
    "discord-rpc": "4.0.1",
    "inquirer": "8.2.5",
    "winston": "3.9.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/discord-rpc": "4.0.5",
    "@types/inquirer": "8.2.6",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.18",
